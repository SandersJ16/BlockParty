{
    "name": "sandersj16/officeblockparty",
    "description": "PHPOffice/PHPSpreadSheet extension for supporting Cell Blocks",
    "authors": [
        {
            "name": "Justin Sanders"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "OfficeBlockParty\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",
        "phpoffice/phpspreadsheet": "^1.10"
    },
    "autoload-dev": {
        "psr-4": {
            "OfficeBlockParty\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.0@dev"
    }
}
